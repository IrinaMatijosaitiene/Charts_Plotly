trace1 = {
  "labels": ['OECD','Central & Eastern Europe (incl. Russia)','Latin America','China','India',
            'Rest of Asia (excl. Japan)','North Africa','Sub-Saharan Africa'], 
  "type": "pie", 
  "values": [0.809,0.537,0.52,0.47,0.311,0.417,0.389,0.22]
